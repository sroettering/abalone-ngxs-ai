    "babel-loader": "^8.0.5",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "p5": "^0.8.0"
  }
